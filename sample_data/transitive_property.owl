            <oboInOwl:hasURI rdf:datatype="&xsd;anyURI">http://purl.org/obo/owl/PMID#PMID_15892874</oboInOwl:hasURI>
          </oboInOwl:DbXref>
        </oboInOwl:hasDbXref>
      </oboInOwl:Definition>
    </oboInOwl:hasDefinition>
    <oboInOwl:hasOBONamespace>relationship</oboInOwl:hasOBONamespace>
